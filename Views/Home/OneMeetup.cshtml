@{
  ViewData["Title"] = "One Meetup";
}
<div class="text-center">
<h1>@ViewBag.OneMeetup.Title</h1>
<p><i>@ViewBag.OneMeetup.Description</i></p>
<h3>@ViewBag.OneMeetup.Date</h3>

@* @foreach (Reservation a in @ViewBag.OneWeddingReservations.UserList)
  {
  <p>@a.UserId</p>
  } *@

<h3 class="my-3">By @ViewBag.OneMeetup.Creator.Name</h3>
@* <h3>Coordinator: @ViewBag.OneMeetupFull.User.Name</h3> *@



<table class="table table-hover m-0 p-0">
  <thead class="border-0">
    <tr>
      <th class="border-0" scope="col">#</th>

      <th class="border-0" scope="col">RSVPS</th>

    </tr>
  </thead>
  <tbody class="bg-light border rounded">

    @foreach (Reservation a in @ViewBag.OneMeetupFull.UserList)
    {
      <tr>

        <th scope="row">@a.User.UserId</th>
        <td>@a.User.Name</td>

      </tr>
    }
  </tbody>
</table>

</div>